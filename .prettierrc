{
  "endOfLine": "auto",
  "printWidth": 100,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^next(/.*)?$",
    "<THIRD_PARTY_MODULES>",
    "^@/types(/.*)?$",
    "^@/config(/.*)?$",
    "^@/lib(/.*)?$",
    "^@/db(/.*)?$",
    "^@/storage(/.*)?$",
    "^@/services(/.*)?$",
    "^@/validations(/.*)?$",
    "^@/hooks(/.*)?$",
    "^@/components(/.*)?$",
    "^@/emails(/.*)?$",
    "^@/modules(/.*)?$",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
